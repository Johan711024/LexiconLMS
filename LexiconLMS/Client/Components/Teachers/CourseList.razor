@using LexiconLMS.Client.Models;
@using LexiconLMS.Client.Services;



<div id="courseList">
    <b>Kurser OBS! Dessa kommer ifrån Databas</b>
    <ul>
        @foreach (var course in courseList)
        {
        <li @onclick="() => doSomething()">@course.Name</li>
        }
        
    </ul>
    <button>Create New</button>
</div>

@code {

    [Parameter]
    public IEnumerable<CourseDto> courseList { get; set; } = new List<CourseDto>();

    private void doSomething()
    {
        Console.WriteLine("From courseList");
    }

}